<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>


<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Theater.ListPage">
   <children>
      <Pane fx:id="paneList" prefHeight="450.0" prefWidth="800.0" style="-fx-background-color: #ffffff;">
         <children>
            <Button fx:id="backBtn" layoutX="376.0" layoutY="399.0" mnemonicParsing="false" onAction="#handleBackBtnOnAction" text="Back" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutY="300.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../decImage/seat.png" />
               </image>
               <effect>
                  <ColorAdjust brightness="-0.6" />
               </effect>
            </ImageView>
         </children>
      </Pane>
   </children>
</StackPane>
